class methodoverloading{
void add(int a,int b)
{
int sum;
sum=a+b;
System.out.println(sum);
}
void add(float a,float b)
{
float sum;
sum=a+b;
System.out.println(sum);
}
public static void main(String[]args)
{
methodoverloading s=new methodoverloading();
s.add(10,20);
s.add(2.6f,4.8f);
}
}

output:307.4
